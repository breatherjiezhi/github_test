	QuerySQL='SELECT \r\n        PUR.PURCHASE_CODE AS purchaseCode, \r\n        ENW.ENWARE_CODE AS enwareCode, \r\n        ENW.DRIVER AS DRIVER,\r\n        ENW.DRIVER_TEL AS TEL,\r\n        ENW.PLATE_NUMBER AS PLATE,\r\n        SUP.SUPPLIER_NAME AS supplierName, \r\n        SUP.Liaison AS Liaison, \r\n        SUP.LIAISONMODE AS LiaisonMode, \r\n        PUR.PLAN_DELIVERY_TIME AS pDeliveryTime, \r\n        PUR.REAL_DELIVERY_TIME AS rDeliveryTime, \r\n        ENWD.PK_MATERIEL as materielKey, \r\n        ENWD.ACHIEVE_NUM as achieveNum, \r\n        m.MATERIEL_CODE as materielCode, \r\n        PRO.PROFESSION_NAME AS Profession, \r\n        m.MATERIEL_NAME AS  materielName, \r\n        m.MATERIEL_FORNAME as materielForName, \r\n        m.MATERIEL_SPECS as  materielSpecs, \r\n        M.PACKAGE_MODE AS packageMode,  \r\n        m.length as length, \r\n        m.width as width, \r\n        m.height as height, \r\n        m.volume as volume, \r\n        m.volume*ENWD.ACHIEVE_NUM AS TOTALVOLUME,  \r\n        m.gross_weight as GW, \r\n        M.gross_weight*ENWD.ACHIEVE_NUM AS TOTALWEIGHT, \r\n        ENWD.REMARKS AS remarks \r\n        FROM ZHL_ENWARE_DE ENWD \r\n        LEFT JOIN ZHL_BASIS_MATERIEL m on m.ID  = ENWD.PK_MATERIEL \r\n        LEFT JOIN ZHL_ENWARE ENW ON ENW.ENWARE_ID = ENWD.PK_ENWARE\r\n        LEFT JOIN ZHL_PURCHASE PUR ON PUR.PURCHASE_ID = ENW.PK_PURCHASE \r\n        LEFT JOIN ZHL_BASIS_SUPPLIER SUP ON SUP.ID = ENW.PK_SUPPLIER \r\n        LEFT JOIN ZHL_BASIS_PROFESSION PRO ON PRO.ID = M.PK_PROFESSION \r\n        WHERE ENW.PK_PURCHASE = ''\r\n        AND ENWD.DEL_FLAG = '0' \r\n        AND ENW.DEL_FLAG = '0''
