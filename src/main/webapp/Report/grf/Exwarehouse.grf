			QuerySQL='SELECT  EXW.EXWARE_CODE AS exwareCode,\r\n        EXW.STOR_ADDRESS AS storAddress,\r\n        EXW.WORKING_TIME AS workingTime,\r\n        PUR2.purchaseList,\r\n        EXW.PLATE_NUMBER AS plateNum,\r\n        EXW.DRIVER AS driver,\r\n        EXW.DRIVER_TEL AS driverTel,\r\n        EXW.CONTAINER_OWNER AS containerOwner,\r\n        EXW.CONTAINER_TYPE AS containerType,\r\n	EXW.CONTAINER_CODE AS containerCode,\r\n	EXW.SEAL_CODE AS sealCode,\r\n        EXW.UPDATE_DATE AS exwareTime,\r\n        M.MATERIEL_CODE AS materielCode,\r\n        M.MATERIEL_NAME AS materielName,\r\n        NUM.materielNum,\r\n        M.MATERIEL_SPECS AS materielSpecs,\r\n        M.PACKAGE_MODE AS packageMode,\r\n        M.LENGTH AS length,\r\n        M.WIDTH AS width,\r\n        M.HEIGHT AS height,\r\n        M.VOLUME AS volume,\r\n        M.GROSS_WEIGHT AS GW,\r\n        M.VOLUME*NUM.materielNum AS TOTALVOLUME,\r\n        M.GROSS_WEIGHT*NUM.materielNum AS TOTALWEIGHT\r\n    FROM ZHL_PACKAGE_DE PACD\r\n    LEFT JOIN ZHL_PACKAGE PAC ON PAC.PACKAGE_ID = PACD.PK_PACKAGE\r\n    LEFT JOIN ZHL_PURCHASE PUR ON PUR.PURCHASE_CODE = PAC.PURCHASE_CODE\r\n    LEFT JOIN ZHL_EXWARE_DE EXWD ON EXWD.PK_PURCHASE = PUR.PURCHASE_ID\r\n    LEFT JOIN ZHL_EXWARE EXW ON EXW.EXWARE_ID = EXWD.PK_EXWARE\r\n    LEFT JOIN ZHL_BASIS_MATERIEL M ON M.ID = PACD.PK_MATERIEL\r\n    LEFT JOIN (SELECT PACD.PK_MATERIEL AS materielKey,\r\n              SUM(PACD.MATERIEL_NUM) AS materielNum\r\n              FROM ZHL_PACKAGE_DE PACD\r\n              group by PACD.PK_MATERIEL) NUM ON NUM.materielKey = M.ID\r\n    LEFT JOIN (select pk_exware,\r\n               replace(WMSYS.WM_CONCAT(pur1.purchase_code),',','；') as purchaseList\r\n               from zhl_exware_de \r\n               left join zhl_purchase pur1 on pur1.purchase_id = pk_purchase \r\n               group by pk_exware) PUR2 ON PUR2.pk_exware = exw.exware_id\r\n    WHERE EXW.EXWARE_ID = '0'\r\n    order by m.materiel_code'
