            a.restaurant_id,
            a.box_code,
            a.box_cn_name,
            a.create_by,
            a.update_by,
            a.create_date,
            a.update_date,
            a.remarks,
            a.del_flag
        FROM pz_box_code a
        WHERE a.ID = #{id} and a.del_flag ='0'
