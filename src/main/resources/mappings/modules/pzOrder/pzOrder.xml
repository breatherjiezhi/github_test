        <if test="null != userId and ''!= userId">AND user_id = #{userId}</if>
        <if test="null != menuId and ''!= menuId">AND menu_id = #{menuId}</if>
        <if test="null != serviceUnitId and ''!= serviceUnitId">AND service_unit_id= #{serviceUnitId}</if>
        <if test="null != restaurantId and ''!= restaurantId">AND restaurant_id = #{restaurantId}</if>
        <if test="null != menuType and ''!= menuType">AND menu_type = #{menuType}</if>
        <if test="null != eatDate and ''!= eatDate">AND eat_date = #{eatDate}</if>
        <if test="null != noEatDate and ''!= noEatDate">AND no_eat_date = #{noEatDate}</if>
